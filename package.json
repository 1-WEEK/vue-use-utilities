{
  "name": "@vueblocks/vue-use-utilities",
  "version": "0.2.10",
  "description": "A collection of Vue composition-api utilities",
  "scripts": {
    "docs:dev:v2": "npm run dev --prefix docs/v2",
    "docs:dev:v3": "npm run dev --prefix docs/v3",
    "docs:build:v2": "npm run build --prefix docs/v2",
    "docs:build:v3": "npm run build --prefix docs/v3",
    "docs:deploy": "sh deploy.sh",
    "build": "rollup -c rollup.config.js",
    "release": "lerna publish",
    "test": "jest --coverage",
    "changelog": "conventional-changelog -p angular -i CHANGELOG.md -s -r 1"
  },
  "sideEffects": false,
  "devDependencies": {
    "@rollup/plugin-replace": "^3.0.0",
    "@types/jest": "^26.0.20",
    "@types/node": "^16.4.0",
    "@vue/test-utils": "^1.2.1",
    "esbuild": "^0.8.29",
    "esbuild-register": "^1.2.1",
    "eslint": "^7.17.0",
    "eslint-plugin-jest": "^24.1.3",
    "esm": "^3.2.25",
    "jest": "^26.6.3",
    "lerna": "^3.22.1",
    "rollup": "^2.40.0",
    "rollup-plugin-dts": "^2.0.1",
    "rollup-plugin-filesize": "^9.1.1",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.29.0",
    "ts-jest": "^26.5.3",
    "typescript": "^4.2.3"
  }
}
